.AUTODEPEND

.PATH.obj = D:\SARIEN\BIN

#		*Translator Definitions*
CC = tcc +SARIEN.CFG
TASM = TASM
TLIB = tlib
TLINK = tlink
LIBPATH = D:\TC\LIB
INCLUDEPATH = D:\SARIEN\SRC\INCLUDE;D:\TC\INCLUDE


#		*Implicit Rules*
.c.obj:
  $(CC) -c {$< }

.cpp.obj:
  $(CC) -c {$< }

#		*List Macros*


EXE_dependencies =  \
 agi.obj \
 agi_v2.obj \
 agi_v3.obj \
 checks.obj \
 cli.obj \
