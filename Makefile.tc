 words.obj \
 fileglob.obj \
 graphics.obj \
 console.obj \
 dummy.obj

#		*Explicit Rules*
d:\sarien\bin\sarien.exe: sarien.cfg $(EXE_dependencies)
  $(TLINK) /v/x/c/L$(LIBPATH) @&&|
